struct StructName 
{
  int property;
 } *variableName;
 
 //inicializando pontiero
 variableName = (struct variableName*) malloc (sizeof (struct StructName));
 //(struct variableName*) type cast
 // malloc (sizeof (struct StructName)) reserva a memÃ³ria e retorna um ponteiro
 
 //acessando property
 variableName->property;
